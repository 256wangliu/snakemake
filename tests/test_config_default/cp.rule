# -*- snakemake -*-
from snakemake.workflow import update_config

c = {
    'cp': {
        'options': '-f',
        },
    }

config = update_config(config, c)

rule cp:
    params: options = config['cp']['options']
    input: '{prefix}.out'
    output: temporary('{prefix}.out.copy')
    shell: 'cp {params.options} {input} {output}'
